/* tab 新样式*/
.tab_flow>ul.title{
	display:table;
	width:100%;
}
.tab_flow>ul.title>li{
	float:none;
	display: table-cell;
	width:1%;
	border-right:0;
	background-color:#f2f2f2;
}
.tab_flow>ul.title>li:last-child{
	border-right:1px #dedede solid;
}
.tab_flow>ul.title>li>a{
	display:block;
}
.tab_flow>ul.title>li>a[disabled]{
	color:#bbb;
	cursor:not-allowed;
}
.tab_flow>ul.title>li>a[disabled]:hover{
	text-decoration:none;
}
.tab_flow>ul.title>li.ho{
