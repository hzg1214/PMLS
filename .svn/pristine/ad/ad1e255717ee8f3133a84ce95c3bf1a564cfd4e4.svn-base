.easyui-accordion.accordion{
	padding:10px;
	border:0;
}
.accordion .accordion-header,
.accordion .accordion-header-selected{
	background:none;
	border-bottom:1px #ccc dashed;
}
.accordion .accordion-header .panel-title{
	height:26px;
	line-height:26px;
	font-size:16px;
	font-weight:bold;
	color:#333;
	position:relative;
	padding-left:10px;
}
.accordion .accordion-header .panel-title:before{
	content:"\20";
	width:4px;
	height:20px;
	display:inline-block;
	background-color:#ba242a;
	position:absolute;
	left:0;
	top:50%;
	margin-top:-10px;
}

.accordion .accordion-header .accordion-collapse{
	margin-top:2px;
	width:43px;
	background:url(../images/accordion_arrows-new.png) no-repeat;
	background-position:-43px 2px;
	
}
.accordion .accordion-header .accordion-collapse.accordion-expand{
	background-position:0 2px;
}

