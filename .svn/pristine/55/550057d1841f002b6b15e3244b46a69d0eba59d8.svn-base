/*单选框*/
.label-radio{
    position: relative;
    color: #989898;
    font-weight: normal;
    min-width: 60px;
    min-height: 20px;
    margin-right: 8px;
    display: inline-block;
    font-size: 14px;
    line-height: 20px;
    border-radius: 4px;
    text-align: center;
    -webkit-box-sizing: border-box!important;
    -moz-box-sizing: border-box!important;
    box-sizing: border-box!important;
}
.label-radio>input[type="radio"]:checked {
    color: #ea4c40;
}
.label-radio:before{
    content: "\20";
    display: block;
    position: absolute;
}
.label-radio:before{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 4;
}
.label-radio>input[type="radio"]{
    -webkit-appearance: none;
    filter:alpha(opacity=0);  
      -moz-opacity:0;  
      -khtml-opacity: 0;  
      opacity: 0; 
    position: absolute;
    top: 0;
    right: 2px;
    bottom: 0;
    left: 0;
    display: block;
    z-index: 0;
    width: 100%;
    height: 100%;
    padding: 13px 5px;
    outline: 0;
    border: 0;
    background-color: rgba(204,204,204,0);
}
.label-radio>span, .label-checkbox>span {
    border: 1px #ccc solid;
    border-radius: inherit;
    display: block;
    overflow: hidden;
    line-height: 28px;
    position: relative;
}
.label-radio>input[type="radio"]:checked + span {
    color: #ea4c40;
    border-color: #ea4c40;
}

