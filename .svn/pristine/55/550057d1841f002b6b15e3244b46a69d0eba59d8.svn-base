/*流程图*/
ol>li{
    padding-left: 30px; 
}
.item{
    position: relative;
    border-left: 3px solid #eee;
    vertical-align: top;
    padding-left: 30px;
}
time{
    position: relative; 
    vertical-align: top;
}
time:before {
    content: "\20";
    position: absolute;
    top: 4px;
    left: -24px;
    width: 10px;
    height: 10px;
    border-radius: 10px;
    border: 2.5px #B5D4F4 solid;
    background-color: #2678ec;
    display: block;
}
.now:before{
    content: "\20";
    position: absolute;
    top: 4px;
    left: -24px;
    width: 10px;
    height: 10px;
    border-radius: 10px;
    border: 2.5px #FF0000 solid;
    background-color: #FF0000;
    display: block;
}
