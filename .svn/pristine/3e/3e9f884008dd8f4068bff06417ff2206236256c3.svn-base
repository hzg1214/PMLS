<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.0</tlib-version>
 <jsp-version>1.2</jsp-version>
 <short-name>t</short-name>
 <uri>/eju-tags</uri>
 <display-name>"自定义标签"</display-name>
 <!-- 选择下拉框 -->
 <tag>
  <name>dictSelect</name>
  <tag-class>cn.com.eju.deal.base.tag.DictSelectTag</tag-class>
  <body-content>jsp</body-content>
  <display-name>下拉选择控件</display-name>
  <small-icon>111</small-icon>
  <description>下拉选择控件</description>
   <attribute>
   <name>field</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
   <description>SELECT表单的Name</description>
  </attribute>
  <attribute>
   <name>extendParams</name>
   <rtexprvalue>true</rtexprvalue>
   <description>扩展参数(json格式)</description>
  </attribute>
  <attribute>
   <name>jsonValue</name>
   <rtexprvalue>true</rtexprvalue>
   <description>额外option参数</description>
  </attribute>
  <attribute>
   <name>id</name>
   <rtexprvalue>true</rtexprvalue>
   <description>SELECT表单的Id</description>
  </attribute>
  <attribute>
   <name>defaultVal</name>
   <rtexprvalue>true</rtexprvalue>
   <description>默认选择的Value</description>
  </attribute>
  <attribute>
   <name>multiple</name>
   <rtexprvalue>true</rtexprvalue>
   <description>是否多选</description>
  </attribute>  
  <attribute>
   <name>type</name>
   <rtexprvalue>true</rtexprvalue>
   <description>控件格式 select|radio|checkbox</description>
  </attribute>
  <attribute>
   <name>xmlkey</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
   <description>自定义key</description>
  </attribute>  
  <attribute>
   <name>nullOption</name>
   <rtexprvalue>true</rtexprvalue>
   <description>空的option</description>
  </attribute>
  <attribute>
   <name>changeEvent</name>
   <rtexprvalue>true</rtexprvalue>
   <description>下拉框改变事件</description>
  </attribute>
  <attribute>
   <name>disable</name>
   <rtexprvalue>true</rtexprvalue>
   <description>是否禁用下拉框</description>
  </attribute>
   <attribute>
   <name>dbparam</name>
   <rtexprvalue>true</rtexprvalue>
   <description>参数</description>
  </attribute>
  <attribute>
   <name>txHeight</name>
   <rtexprvalue>true</rtexprvalue>
   <description>输入框高</description>
  </attribute>
  <attribute>
   <name>txWidth</name>
   <rtexprvalue>true</rtexprvalue>
   <description>输入框宽</description>
  </attribute>
  <attribute>
   <name>classvalue</name>
   <rtexprvalue>true</rtexprvalue>
   <description>输入框宽</description>
  </attribute>
   <attribute>
   <name>required</name>
   <rtexprvalue>true</rtexprvalue>
   <description>是否必须</description>
  </attribute>
  <attribute>
   <name>editable</name>
   <rtexprvalue>true</rtexprvalue>
   <description>是否可编辑</description>
  </attribute>
    <attribute>
   <name>panelHeight</name>
   <rtexprvalue>true</rtexprvalue>
   <description>下拉框高度</description>
  </attribute>
 </tag>
 <tag>
  <name>RecomboBox</name>
  <tag-class>cn.com.eju.deal.base.tag.RemoteComboBoxTag</tag-class>
  <body-content>jsp</body-content>
  <description>下拉选择框</description>
  <attribute>
   <name>name</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
   <description>控件名称</description>
  </attribute>
  <attribute>
   <name>url</name>
   <required>true</required>
   <rtexprvalue>true</rtexprvalue>
   <description>远程数据</description>
  </attribute>
  <attribute>
   <name>id</name>
   <rtexprvalue>true</rtexprvalue>
   <description>ID对应字段</description>
  </attribute>
  <attribute>
   <name>text</name>
   <rtexprvalue>true</rtexprvalue>
   <description>text对应字段</description>
  </attribute>
  <attribute>
   <name>width</name>
   <rtexprvalue>true</rtexprvalue>
   <description>width对应字段</description>
  </attribute>
  <attribute>
   <name>listWidth</name>
   <rtexprvalue>true</rtexprvalue>
   <description>listWidth对应字段</description>
  </attribute>
  <attribute>
   <name>listHeight</name>
   <rtexprvalue>true</rtexprvalue>
   <description>listHeight对应字段</description>
  </attribute>
 </tag>
</taglib>
