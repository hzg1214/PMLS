/*寮瑰嚭妗�鎶ュ璇︽儏*/
.fast-filing-step .tran_step>li{height:45px;}
.fast-filing-step .tran_step>li.active{background-color:#337ab7;}
.fast-filing-step .tran_step>li.active + li:before{background-color:#337ab7;}
.fast-filing-step .tran_step>li + li:before{height:35px; width:35px;}
.tran_step{display:table; padding:0;}
.tran_step>li{display:table-cell;width:1%;text-align:center;vertical-align:middle;height:46px;line-height:46px;color:#fff;position:relative;background-color:#c2c6c7;}
.tran_step > li:first-child{border-radius:3px 0 0 3px;}
.tran_step > li:last-child{border-radius:0 3px 3px 0;}
.tran_step>li + li:before{content:"\20";display:block;width:28px;height:28px;border-width:2px 2px 0 0;border-color:#fff;border-style:solid;position:absolute;top: 5px;left: -17px;-webkit-transform:rotate(45deg);transform:rotate(45deg);}
.tran_step>li.done{background-color:#ec9993;}
.tran_step>li.done + li:before{background-color:#ec9993;}
.tran_step>li.active{background-color:#337ab7;}
.tran_step>li.active:before{background-color:#337ab7;}
.tran_step>li.active + li:before{background-color:#337ab7;}
.y-bbxqdate li{width: 16%;text-align: center;margin-top: -5px;font-size:13px; line-height:28px; margin-bottom:10px; list-style:none; float:left;}
.y-modal-body{text-align:center; font-size:19px;height: 110px;line-height: 110px;}

.tran_step a{ color:#fff;}
.step-item>a>span{display:block;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.step-item>a>span:before,
.step-item>a>span:after{color:#a1a1a1;font-size:13px;position:absolute;left:20px;top:80px; left:0;right:0;}
.step-item>a>span:before{content:attr(data-name);top:65%;}
.step-item>a>span:after{content:attr(data-date);top:70%;}

