@charset "utf-8";
/* for bootstrap */
body{
	font-family: "Helvetica Neue", Helvetica, Verdana, "STHeitiSC-Light", "STHeitiSC-Medium", "Microsoft Yahei",  Arial, sans-serif;
}
.navbar-fixed-top ~ .container{
	padding-top:60px;
} 
.container{
	font-size:12px;
}
.page-header{
	margin-top:20px;
}
.page-header>h1,
.page-header>h2,
.page-header>h3,
.page-header>h4{
	display:inline-block;
	vertical-align:middle;
}
.page-header:after{
	content:"\20";
	clear:both;
	width:0;
	height:0;
	display:block;
}
.page-header>nav{
	display:inline-block;
}

.nav-page-header{
	margin-left:25px;
	vertical-align:middle;
}

.nav-filter{
	float:right;
	/*padding-top:15px;*/
	padding-top:10px;
}
.nav-filter>a{
	padding:4px 8px;
	border-radius:3px;
	text-decoration:none;
}
.nav-filter>a:hover{
	
	background-color:#eee;
}
.nav-filter>a.active{
	background-color:#34AC26;
	color:#fff;
}

.page-content{
	clear:both;
}

/* pagination */
.pagination-info,
.pagination-jumper{
	display:inline-block;
	margin-top: 15px;
    margin-bottom: 15px;
}
.pagination-info,
.pagination-jumper,
.pagination-info + .pagination{
	vertical-align:middle;
}
.pagination-info{
	margin-right:40px;
}
.pagination-jumper{
	margin-left:40px;
}
.pagination-jumper>.form-control{
	display:inline-block;
	width:auto;
}



.footer{
	padding-top: 40px;
	padding-bottom: 40px;
	margin-top: 40px;
	border-top: 1px solid #eee;
}
footer{
	padding-top:40px;padding-bottom:40px;margin-top:100px;color:#767676;text-align:center;border-top:1px solid #e5e5e5;
}
.footer-links{padding-left:0;margin-top:20px}
.footer-links li{display:inline;padding:0 2px}
.footer-links li:first-child{padding-left:0}
@media (min-width:768px){
	.footer p{margin-bottom:0}
}



.form-control[disabled], .form-control[readonly], fieldset[disabled] .form-control{
	background-color: #fafafa;
}
.control-label.important:after{
	content:"*";
	vertical-align:middle;
	color:#E9232E;
	padding:5px 5px 0 5px;
}


.nav-tabs-header{
	
}
.nav-tabs-header>li{
	display:inline-block;
	width:auto;
	float:left;
}
.nav-tabs-header>li>a{
	padding:10px 25px;
}

.nav-tabs-header + .page-content{
	padding-top:15px;
	padding-bottom:15px;
}

.form-inline + .form-inline{
	margin-top:15px;
}

/* input-tag */
.input-tag{
	width:130px;
	position:relative;
}
.input-tag>.form-control{
	padding-right:30px;
}
.input-tag-inline{
	display:inline-block;
	vertical-align:middle;
}
.input-tag-inline + .input-tag-inline{
	margin-left:10px;
}
.btn-remove-input-tag{
	position:absolute;
	right:5px;
	top:50%;
	margin-top:-10px;
	width:20px;
	height:20px;
	border-radius:100%;
	background-color:#bbb;
	cursor:pointer;
	display:block;
}
.btn-remove-input-tag:before,
.btn-remove-input-tag:after{
	content:"\20";
	position:absolute;
	left:50%;
	top:50%;
	background-color:#fff;
	-webkit-transform:rotate(45deg);
	transform:rotate(45deg);
}
.btn-remove-input-tag:before{
	width:10px;
	height:2px;
	margin-top:-1px;
	margin-left:-5px;
}
.btn-remove-input-tag:after{
	width:2px;
	height:10px;
	margin-top:-5px;
	margin-left:-1px;
}
.btn-remove-input-tag:hover{
	background-color:#DB1B34;
}

.input-inline,
.label-inline{
	display:inline-block;
}
.input-inline-sm{
	width:120px;
}
.input-inline-xsm{
	width:80px;
}
.label-inline{
	font-weight:normal;
}
.label-inline + .input-inline{
	margin-left:20px;
}


.pagination{
	margin-top:15px;
	margin-bottom:15px;
}

.table + .pagination,
.table + nav>.pagination{
	margin-top:0;
}
/*header*/
.navbar-fixed-top{
	border-width:1px 0 4px;
	margin-bottom:0;
	min-height:35px;
}
.navbar-fixed-top:before{
	content:"\20";
}
.navbar-logo{
	float:left;
}
.navbar-logo>a{
	float:left;
	color:#fff;
	margin-top:7px;
	text-decoration:none;
}
.navbar-logo>a.navbar-header-link{
	font-size:14px;
	margin-top:4px;
}
/* .navbar-nav>li>a{
	padding:10px 15px;
} */

.navbar-header-link:before{
	content:"\20";
	display:inline-block;
	width:1px;
	height:20px;
	background-color:#70a2cd;
	margin:0 10px;
	position:relative;
	top:5px;
}
.navbar-inverse{
	background-color:#327ab7;
	border-bottom-color:#029be3;
	border-top-color:#fff;
}
.navbar-inverse .navbar-nav>li>a{
	color:#fff;
}
.navbar-inverse .navbar-nav>.active>a, .navbar-inverse .navbar-nav>.active>a:focus, .navbar-inverse .navbar-nav>.active>a:hover{
	background-color:#029be3;
}
.navbar-inverse .navbar-nav>.open>a, .navbar-inverse .navbar-nav>.open>a:focus, .navbar-inverse .navbar-nav>.open>a:hover{
	background-color:#029be3;
}
.glyphicon-envelope:before{
	color:#fff;
}
.gly-num{
	margin-left:3px;
}
.navbar-nav-data>li>a{
	padding:10px 6px;
}
.navbar-li-last .glyphicon-user:before{
	color:#3e4d6b;
	font-size:20px;
}
.navbar-li-last .glyphicon-user{
	border:2px solid #fff;
	background-color:#cdcccd;
}
.navbar-nav-data>li.navbar-li-last>a{
	padding-top:0;
	position:relative;
	top:6px;
	cursor:default;
}
.navbar-li-last .gly-name{
	position:relative;
	top:-3px;
	margin-right:5px;
}


.text-overflow{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	width:100%;
}
span.text-overflow{
	display:block;
}
th>.text-overflow,
td>.text-overflow{
	width:inherit;
}


/*鐓х墖鏍峰紡begin*/
.add_listings_cont button,
.add_listings_cont input,
.add_listings_cont ol,
.add_listings_cont ul,
.add_listings_cont li,
.add_listings_cont dl,
.add_listings_cont dt,
.add_listings_cont dd{list-style: none; margin:0;padding:0; font:12px/1 microsoft yahei,tahoma,Arial,"\5b8b\4f53",Helvetica,sans-serif;}
.add_listings_cont dl{padding:5px 15px; color:#333; margin-bottom:5px;}
.add_listings_cont dl:after,.y_fyrw dl:after{content:"\20"; display: block; height: 0;clear: both; visibility: hidden;}
.add_listings_cont dl dt{text-align:right; margin-right:10px; float:left; line-height:28px;}
.add_listings_cont dl dt:first-child{min-width:60px;}
.add_listings_cont dl dt s{color:#ea4c40; margin-right:4px; visibility:hidden;}
.add_listings_cont dl dt s.cur{visibility:inherit;}
.add_listings_cont dl dd{float:left;}
.add_listings_cont .w_168{width:168px;}
.add_listings_cont dl dd .list_checkbox,.add_listings_cont dl dd .list_radio{display:inline-block; width:450px; height:25px; overflow:hidden; float:left;}
.add_listings_cont dl dd .label_radio,.add_listings_cont dl dd .label_checkbox{margin-bottom:10px;}
.add_listings_cont dl dd span button{float:left; margin:0 15px 15px 0; min-width:60px; padding:0;}
.add_listings_cont dl dd .add_fy_btn{display:block; font-size:14px; margin-left:15px; overflow:hidden; text-decoration:none;}
.add_listings_cont dl dd .add_fy_btn s{margin:3px 10px 0 0; float:left;}
.list_photo_uploader{overflow:hidden; margin-bottom:10px;}
.list_photo_uploader,
.list_photo_uploader>li{
	list-style:none;
	margin:0;
	padding:0;
}
.list_photo_uploader li{float:left; margin-right:10px; position:relative; width:156px; height:116px; border:1px solid #d8dcdd; overflow:hidden;}
.list_photo_uploader li img{width:156px; height:116px;padding:0;}
.list_photo_uploader li > .btn_remove_photo{position:absolute; right:2px; top:2px; width:20px; height:20px; background-color:#666; border-radius:10px; cursor:pointer; z-index:10; transform:rotate(45deg); -webkit-transform:rotate(45deg);}
.btn_remove_photo:after,.btn_remove_photo:before{content:"\20"; display: block;background-color: #fff;position: absolute;left: 50%;top: 50%;border-radius:2px;}
.btn_remove_photo:before{width:14px; height:1px; margin-left:-7px;}
.btn_remove_photo:after{width:1px; height:14px; margin-top:-7px;}
.list_photo_uploader li .set-cover{cursor:pointer; position:absolute; width:100%; height:25px; padding-left:10px; line-height:25px; bottom:0; left:0; background:rgba(0,0,0,0.6); color:#fff; text-decoration:none; box-sizing:border-box; -webkit:box-sizing,border-box; display:none;}
.list_photo_uploader li:hover .set-cover{display:block;}
.list_photo_uploader li.item_cover .set-cover{display:none;}
.form-hide-set-cover .list_photo_uploader li .set-cover{display:none;}

.cover_bg{position:absolute; top:0; left:0; width:34px; height:34px; background:url(../images/cover_bg.png) no-repeat 0 0; display:none;}
.list_photo_uploader li.item_cover .cover_bg{display:block;}

.list_photo_uploader li > s.s_add_btn{display:block;}
.list_photo_uploader li > s.s_add_btn:before,
.list_photo_uploader li > s.s_add_btn:after{
	content:"\20"; 
	display:block;
	position:absolute;
	top:50%;
	left:50%;
	background-color:#d8dcdd; 
}
.list_photo_uploader li > s.s_add_btn:before{width:28px; height:1px; margin-left:-14px;}
.list_photo_uploader li > s.s_add_btn:after{width:1px; height:28px;margin-top:-14px;}
.list_photo_uploader li > .file_control{position:absolute; top:0; left:0; font-size:800px; cursor:pointer; z-index:10; line-height:normal; filter: alpha(opacity=0); opacity:0;}
.list_photo_uploader li > .btn-add-photo{width:100%;height:100%;}
button.drawbtn{border:none; margin-left:5px; color:#145abf; background-color:transparent;}
