<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<%@include file="/WEB-INF/page/common/taglib.jsp" %>
<form>
    <div class="fs14" role="main">
        <div class="row">
            <div class="page-content" style="">
                <h4><strong>重置管理密码</strong></h4>
                <ul class="list-inline form-inline">
                    <li>
                        <div class="form-group">
                            <label class="fw-normal w100 text-right">新密码</label>
                            <input name="firstPwd" type="password" class="form-control w300" id="J_newPassword" value="" maxlength="10">
                        </div>
                    </li>
                </ul>
                <ul class="list-inline form-inline">
                    <li>
                        <div class="form-group">
                            <label class="fw-normal w100 text-right">确认密码</label>
                            <input name="secondPwd" type="password" class="form-control w300" id="J_affirmPassword" value="" maxlength="10">
                        </div>
                    </li>
                </ul>
                <ul class="list-inline form-inline pdb20" id="J_errorPop" style="display: none;">
                    <li>
                        <div class="form-group">
                            <label class="fw-normal w100 text-right"></label>
                            <span class="fc-warning">两次密码不同，请重新输入！</span>
                        </div>
                    </li>
                </ul>
            </div>

        </div>
    </div>
</form>
