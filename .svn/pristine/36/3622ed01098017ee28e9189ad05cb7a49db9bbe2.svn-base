/*  左边栏  */
.article {
	height: auto;
	
	padding-bottom: 20px;
}
.article:after {
	content: "\20";
	clear: both;
	display: block;
	width: 0;
	height: 0;
}
.article, .article>.aside {
	min-height: 600px;
}
.article>.container {
	border: 1px solid #d5d5d5;
	overflow: hidden;
}
.aside {
	width: 179px;
	height: auto;
	background: none;
	float: left;
	
}
.article>.content {
	margin-top: 0;
	display: block;
	background-color: #fff;
	padding-bottom: 20px;
	
}
.aside ul li:hover {
	background: #ba242a;
}
.aside ul li:hover a {
	color: #fff;
}
.aside p:hover {
	background: #ba242a;
	color: #fff;
	
}
.aside p:hover a{
	text-decoration:none;
}
.men a {
