	padding-right:15px;
}
.prison_infor {
	float: left;
	margin-left: 30px;
	position:relative;
}

.prison {
	padding-top:16px;
	height:20px;
	line-height:20px;
	font-size: 14px;
	font-weight: normal;
	background: url(../images/prison.png) no-repeat left 22px;
	padding-left: 22px;
	vertical-align: top;
}
a.prison {
	line-height:24px;
}
.msg {
	background: url(../images/MSG.png) no-repeat center;
}
.msg_new{
	position: absolute;
    display: block;
    width: 13px;
    height: 13px;
    background-color: #d01d23;
    border-radius: 13px;
    top: 14px;
    right: 45px;
}
.msg_new>a{
	position: absolute;
    top: -21px;
    right: 3px;
    font-size: 11px;
    color: #fff;
	
}
.msg_new>a:hover{
	color:#fff!important;
	text-decoration:none;
}
.prison:hover {
	font-weight: normal
}
.msg, .out {
	width: 18px;
	height: 14px;
	display: inline-block;
	margin-left: 15px;
}
.out {
	width: 12px;
	height: 14px;
	background: url(../images/out.png) no-repeat center;
	background-color: #e7e7e7;
	padding: 0 8px;
}
.out:hover{
	background: url(../images/out_hover.png) no-repeat center;
}

.out, .msg, .prison {
	display: inline-block;
}
.out, .msg{
	height: 54px;
}

/*  面包屑  */
.breadcrumb {
	height: 30px;
	line-height: 30px;
